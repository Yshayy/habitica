    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Storybook",
        "request": "launch",
        "cwd": "${workspaceFolder}/website/client",
        "runtimeArgs": [
          "run-script",
          "storybook:serve"
        ],
        "runtimeExecutable": "npm",
        "type": "pwa-node"
      },
        {
            "name": "Launch & Debug server",
            "type": "pwa-node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run-script", "debug"]
          },
          {
            "name": "Launch Client",
            "type": "pwa-node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run-script", "client:dev"]
          },
          
          
    ]
}